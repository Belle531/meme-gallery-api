{
  "name": "meme-gallery-api",
  "version": "1.0.0",
  "description": "A REST API for managing memes with PostgreSQL database, deployable on Render",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "nodemon --watch src --exec 'node --loader ts-node/esm src/index.js --no-warnings'",
    "start": "node dist/index.js"
  },
  "keywords": [
    "api",
    "memes",
    "express",
    "postgresql",
    "render"
  ],
  "author": "Cassandra Moore",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.9.0",
    "@types/pg": "^8.15.5",
    "netlify-cli": "^17.38.1",
    "prisma": "^6.17.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
